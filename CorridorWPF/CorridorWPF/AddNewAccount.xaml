<Window x:Class="CorridorWPF.AddNewAccount"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CorridorWPF"
        mc:Ignorable="d"
        Title="AddNewAccount" Height="300" Width="300">
    <Grid>
        <Button x:Name="btn_AddUser" Content="Add" HorizontalAlignment="Left" Margin="207,239,0,0" VerticalAlignment="Top" Width="75" Click="btn_AddUser_Click"/>
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Height="23" Margin="90,105,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="textBox_Copy" HorizontalAlignment="Left" Height="23" Margin="90,165,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Label x:Name="label" Content="Username" HorizontalAlignment="Left" Margin="90,74,0,0" VerticalAlignment="Top"/>
        <Label x:Name="label_Copy" Content="Password" HorizontalAlignment="Left" Margin="90,134,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
